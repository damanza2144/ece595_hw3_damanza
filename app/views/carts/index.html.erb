<div class="listing">
	<h1>Listing Carts</h1>
	<table>
	  <tr class="table_header">
		<td>Cart ID</td>
		<td></td>
	  </tr>
	 <% @carts.each do |cart| %>
	  <tr class="<%= cycle('list_line_odd', 'list_line_even') %>">
		<td><%= cart.id %></td>
		<td class="list_actions">
			<%= link_to 'Show', cart %><br />
			<%= link_to 'Edit', edit_cart_path(cart) %><br />
			<%= link_to 'Destroy', cart, confirm: 'Are you sure?', method: :delete %>
		</td>
	  </tr>
	 <% end %>
	</table>
</div>
<br />
<%= link_to 'New Cart', new_cart_path %>
<p><%= will_paginate @carts %></p>
